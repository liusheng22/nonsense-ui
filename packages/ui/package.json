{
  "name": "nonsense-ui",
  "version": "1.0.2",
  "description": "一个毫无用处、专门折磨用户的 Vue 3 荒诞组件实验室。内置一系列反人类、反直觉的整蛊交互。",
  "keywords": [
    "vue",
    "vue3",
    "nonsense-ui",
    "整蛊",
    "反人类",
    "无厘头",
    "反直觉",
    "毫无体验",
    "实验室"
  ],
  "homepage": "https://nonsense-ui.lius.me/",
  "bugs": {
    "url": "https://github.com/liusheng22/nonsense-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liusheng22/nonsense-ui.git"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build",
    "typecheck": "vue-tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  }
}
